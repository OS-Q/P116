{
  "build": {
    "core": "sduino",
    "extra_flags": " -DSTM8L052",
    "f_cpu": "16000000L",
    "cpu": "stm8",
    "mcu": "STM8L052C6t6",
    "variant": "stm8sdisco"
  },
  "debug": {
    "svd_path": "STM8L052C6.svd",
    "openocd_target": "stm8l",
    "default_tools": [
      "stlink"
    ],
    "onboard_tools": [
      "stlink"
    ]
  },
  "frameworks": [
    "arduino",
    "spl"
  ],
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 32768,
    "protocol": "stlink",
    "protocols": [
      "serial",
      "stlinkv21",
      "stlinkv2"
    ]
  },
  "name": "ST STM8S-DISCOVERY",
  "url": "https://www.st.com/en/evaluation-tools/stm8s-discovery.html",
  "vendor": "ST"
}
